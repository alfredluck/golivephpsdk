{
    "name": "alfredluck/golivephpsdk",
    "description": "golive php api sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "alfredluck",
            "email": "yangjun0506@icloud.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Alfredluck\\Golivephpsdk\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "repositories": {
        "weather": {
            "type": "path",
            "url": "../golivephpsdk"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alfredluck\\Golivephpsdk\\ServiceProvider"
            ]
        }
    }
}
